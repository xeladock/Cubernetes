{
  "kind": "Deployment",
  "apiVersion": "extensions/v1beta1",
  "metadata": {
    "name": "redis-slave",
    "creationTimestamp": null,
    "labels": {
      "io.kompose.service": "redis-slave"
    },
    "annotations": {
      "kompose.cmd": "kompose convert -j",
      "kompose.version": "1.9.0 (093c756)"
    }
  },
  "spec": {
