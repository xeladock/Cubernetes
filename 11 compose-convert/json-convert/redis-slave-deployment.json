        }
      },
      "spec": {
        "containers": [
          {
            "name": "redis-slave",
            "image": "gcr.io/google_samples/gb-redisslave:v1",
            "ports": [
              {
                "containerPort": 6379
              }
            ],
            "env": [
              {
                "name": "GET_HOSTS_FROM",
                "value": "dns"
              }
            ],
            "resources": {}
          }
        ],
        "restartPolicy": "Always"
      }
    },
    "strategy": {}
  },
  "status": {}
